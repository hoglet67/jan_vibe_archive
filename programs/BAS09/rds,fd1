   10 REM > RDS
   20 
   30 MODE32:OFF
   40 A=0:Z=384:FORN%=0TO63
   50 X%=640+Z*SINRADA:Y%=450+Z*COSRADA:R%=2*(64-N%):A=A+10:Z=Z-6
   60 GCOLN%:CIRCLE FILL X%,Y%,R%+8:NEXT
   70 
   80 FORX%=0TO1280STEP2
   90 FORY%=0TO1023STEP2
  100 D%=128-POINT(X%,Y%)*2
  110 PX%=X%-D%-128
  120 IF PX%<0 THEN
  130 C%=RND(2)
  140 ELSE
  150 C%=2:IFPOINT(PX%,Y%)=0 C%=1
  160 ENDIF
  170 GCOL0:IFC%=2 GCOL63
  180 POINT X%,Y%
  190 NEXT,
