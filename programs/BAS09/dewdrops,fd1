   10 REM >DEWDROPS
   20 
   30 MODE25:OFF
   40 FORN%=1TO60000
   50 PX%=427*(RND(1)+RND(1)+RND(1)):PY%=320*(RND(1)+RND(1)+RND(1))
   60 POINT PX%,PY%:NEXT
   70 
   80 REPEAT
   90 K%=0:R%=RND(32)+16:X%=RND(1279-2*R%)+R%:Y%=RND(960-2*R%)+R%
  100 FORYL%=-R%TOR%STEP2:XL%=SQR(R%*R%-YL%*YL%)
  110 GCOL0:FORN%=-XL%TOXL%STEP2
  120 IF POINT(X%+N%,Y%+YL%)<>0 POINTX%+N%,Y%+YL%:K%+=4
  130 NEXT,:GCOL1:IF K%>0 CIRCLE FILL X%,Y%,SQR(K%/PI)
  140 UNTIL0
