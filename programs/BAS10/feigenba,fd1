   10 REM > FEIGENBAUM
   20 
   30 MODE20:OFF
   40 FORN%=0TO15:COLOURN%,16*N%,16*N%,16*N%:NEXT
   50 Y=2.9:X=.7:F=1020/(4-Y)
   60 FORN%=1TO100:X=Y*X*(1-X):NEXT
   70 REPEAT
   80 Y=Y+.000001:X=Y*X*(1-X)
   90 PX%=1280*X:PY%=(Y-2.9)*F
  100 C%=POINT(PX%,PY%)+1:IFC%>15 C%=15
  110 GCOLC%:POINT PX%,PY%
  120 UNTIL Y>=4
