   10 REM >WINDOWS
   20 
   30 MODE31:OFF:PROCA(200,0,1198,1198,1):END
   40 DEFPROCA(X%,Y%,W%,H%,C%)
   50 LOCALW2%,H2%
   60 GCOLC%:RECTANGLE X%,Y%,W%,H%
   70 IF W%>20 AND H%>20 THEN
   80 W2%=(W%-12)/2:H2%=(H%-12)/2
   90 PROCA(X%+4,Y%+4,W2%,H2%,C%+1)
  100 PROCA(X%+4,Y%+H2%+8,W2%,H2%,C%+1)
  110 PROCA(X%+W2%+8,Y%+4,W2%,H2%,C%+1)
  120 PROCA(X%+W2%+8,Y%+H2%+8,W2%,H2%,C%+1)
  130 ENDIF
  140 ENDPROC
