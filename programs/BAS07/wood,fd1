   10 REM > WOOD
   20 
   30 MODE12:OFF
   40 FORN%=0TO15:COLOUR N%,13*N%+32,10*N%,4*N%:NEXT:K%=RND(29)
   50 FORN%=1TO1279STEP2:K%=(K%+RND(3)+28)MOD30
   60 GCOL ABS(K%MOD30-14):POINT N%,0:NEXT
   70 REPEAT:MOVE0,0:MOVE1279,1024:PLOT&BD,0,4
   80 X%=RND(1279):C%=POINT(X%,4)
   90 REPEAT:C1%=RND(3)-2+C%:UNTIL C1%>=0 AND C1%<=15:GCOL C1%
  100 R%=(RND(2)-1)*2:MOVE0,4:MOVEX%,4:PLOT&BE,-R%,0
  110 MOVEX%,4:MOVE1279,4:PLOT&BE,X%+2-R%,0:POINT X%,0:UNTIL0
