   10 REM > BANG
   20 
   30 MODE132:MODE4:OFF:S1%=1:S2%=2:K%=100:DIM DX(K%),DY(K%)
   40 
   50 ONERRORGOTO140
   60 REPEAT
   70 PX%=RND(1280):PY%=RND(1024):T=0
   80 FORN%=1TOK%:D=RADRND(360):S=80*RND(1):DX(N%)=S*SIND:DY(N%)=S*COSD:NEXT
   90 REPEAT:F%=0:SYS 6,112,S1%:SYS 6,113,S2%:WAIT:CLS:SWAP S1%,S2%
  100 FORN%=1TOK%:X=PX%+DX(N%)*T:Y=PY%+DY(N%)*T-4.5*T*T
  110 CIRCLEFILLX,Y,8:IF Y>0 F%=1
  120 NEXT:T+=.4:UNTILF%=0:UNTIL0
  130 
  140 SYS 6,112,S1%:REPORT:PRINT" at line "STR$ERL:ON
