   10 REM > RUNDDANS
   20 
   30 MODE128:MODE0:OFF:ORIGIN 640,512
   40 SYS"OS_UpdateMEMC",64,64
   50 S1$="1":S2$="2":A%=270
   60 
   70 ONERRORGOTO170
   80 REPEAT
   90 OSCLI"FX112,"+S1$:OSCLI"FX113,"+S2$:WAIT:CLS:SWAP S1$,S2$
  100 
  110 A%=(A%+1)MOD360:AT=RADA%:K1%=240*(1+SINAT):K2%=480-K1%
  120 FORN%=09TO360STEP09:T=RADN%:Z=ABS(K1%*COS(3*T+4*AT)+K2%)
  130 CIRCLE Z*SINT,Z*COST,Z/20+4:NEXT
  140 
  150 UNTIL0
  160 
