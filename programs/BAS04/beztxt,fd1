   10 REM > BEZTXT
   20 
   30 MODE0
   40 DIM X(10),Y(10)
   50 INPUT LINE"Indtast tekst "T$:CLS:OFF
   60 PROCTXT(0,800,4,4,6,T$)
   70 END
   80 
   90 DEFPROCTXT(PX%,PY%,ZX%,ZY%,B%,T$)
  100 LOCAL A%,I%,K%,N%,X1%,Y1%,X(),Y()
  110 DIM X(10),Y(10):PX%+=B%
  120 FORI%=1TOLENT$:B$=MID$(T$,I%,1)
  130 K%=FNF(B$)
  140 IF K%<>0 THEN
  150 RESTORE K%
  160 REPEAT
  170 READ A%
  180 IF A%<>0 THEN
  190 FORN%=0TOA%:READ X1%,Y1%
  200 X(N%)=ZX%*X1%+PX%
  210 Y(N%)=ZY%*Y1%+PY%
  220 NEXT
  230 PROCBEZIER(X(),Y(),A%,64,B%)
  240 ENDIF
  250 UNTIL A%=0
  260 ENDIF
  270 PX%=PX%+24*ZX%
  280 IF PX%>=(1280-24*ZX%) PX%=B%:PY%=PY%-42*ZY%
  290 NEXT
  300 ENDPROC
  310 
  320 DEFPROCBEZIER(X(),Y(),D%,P%,Z%)
  330 LOCAL F(),I%,J%,U1,U2,K,PX,PY
  340 DIM F(D%)
  350 IF D%=1 THEN
  360 FORI%=0TOP%:J%=P%-I%
  370 PX=(X(0)*J%+X(1)*I%)/P%:PY=(Y(0)*J%+Y(1)*I%)/P%
  380 CIRCLE FILL PX,PY,Z%:NEXT
  390 ENDPROC
  400 ENDIF
  410 FOR I%=0TOD%:F(I%)=1
  420 FOR J%=1 TO D%:F(I%)=F(I%)*J%:NEXT
  430 FOR J%=1 TO I%:F(I%)=F(I%)/J%:NEXT
  440 FOR J%=1 TO D%-I%:F(I%)=F(I%)/J%:NEXT
  450 NEXT
  460 MOVE X(0),Y(0)
  470 FORJ%=0TOP%:U1=J%/P%:U2=1-U1:PX=0:PY=0
  480 FORI%=0TOD%:K=F(I%)*U1^I%*U2^(D%-I%):PX+=X(I%)*K:PY+=Y(I%)*K
  490 NEXT:CIRCLE FILL PX,PY,Z%:NEXT
  500 ENDPROC
  510 
  520 DEFFNF(T$)
  530 LOCALP%,T%,F%,S$
  540 S$="ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]0123456789.,!?-+/*=()"
  550 IF INSTR(S$,T$)=0 THEN =0
  560 P%=PAGE:T%=ASCT$:F%=0
  570 REPEAT:IFP%?4<>244THEN590
  580 IFP%?5=T% F%=1
  590 P%=P%+P%?3:UNTILF%=1
  600 =P%?2+256*P%?1
  610 
  620 
  630 REMA
  640 DATA 1,0,0,0,16
  650 DATA 2,0,16,0,32,8,32
  660 DATA 2,8,32,16,32,16,16
  670 DATA 1,16,16,16,0
  680 DATA 1,0,16,16,16
  690 DATA 0
