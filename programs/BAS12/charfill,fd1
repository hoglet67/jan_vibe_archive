   10 REM > CHARFILL
   20 
   30 MODE31
   40 DIM A$(64)
   50 FORN%=32TO127
   60 CLS:MOVE0,15:VDU5:VDUN%:VDU4
   70 C%=0
   80 FORY%=0TO15STEP2
   90 FORX%=0TO15STEP2
  100 IFPOINT(X%,Y%)<>0C%+=1:POINTX%,Y%
  110 NEXT,
  120 PRINTC%
  130 A$(C%)+=CHR$(N%)
  140 NEXT
  150 CLS
  160 FORN%=0TO64:PRINTN%" "A$(N%):NEXT
