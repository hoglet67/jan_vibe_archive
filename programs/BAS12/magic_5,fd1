   10 REM > MAGIC_5
   20 
   30 MODE31:@%=3
   40 DIM A%(5,5)
   50 REPEAT:DX%=(RND(2)+1):DY%=(RND(2)+1):UNTILDX%<>DY%
   60 FORM%=1TO5
   70 REPEAT:X%=RND(5):Y%=RND(5):UNTILA%(X%,Y%)=0
   80 FORN%=1TO5:A%(X%,Y%)=M%:X%=(X%+DX%)MOD5+1:Y%=(Y%+DY%)MOD5+1:NEXT,
   90 FORI%=1TO5:FORJ%=1TO5:PRINTA%(J%,I%)+5*(A%(I%,J%)-1);:NEXT:PRINT':NEXT
