   10 MODE4
   20 REPEAT
   30 CLS
   40 FORN%=0TO100
   50 X%=RND(1276):Y%=RND(1020)
   60 L%=RND(100)+32
   70 MOVEX%,Y%
   80 IFRND(1)>.5THEN
   90 DRAWL%*SGN(RND(1)-.5)+X%,Y%
  100 ELSE
  110 DRAWX%,L%*SGN(RND(1)-.5)+Y%
  120 ENDIF
  130 NEXT
  140 X%=0:Y%=0:DX%=4:DY%=4
  150 REPEAT
  160 F%=0
  170 X1%=X%+DX%:IFPOINT(X1%,Y%)<>0 DX%=-DX%:F%=F%+1:GOTO170
  180 Y1%=Y%+DY%:IFPOINT(X%,Y1%)<>0 DY%=-DY%:F%=F%+1:GOTO180
  190 X%=X1%:Y%=Y1%:PLOT69,X%,Y%
  200 UNTILF%>1
  210 UNTIL0
