   10 REM > 3DNAVN
   20 
   30 SYS"OS_UpdateMEMC",64,64
   40 ONERRORGOTO650
   50 DIM XW(200),YW(200),ZW(200)
   60 DIM XR(200),YR(200),ZR(200)
   70 DIM XS(200),YS(200)
   80 DIM plot(200),point(200)
   90 MODE0:MODE128:SP%=1
  100 ORIGIN 640,512:OFF
  110 COLOUR 0,4
  120 XROT=0:YROT=0:ZROT=0
  130 C%=0:D%=0
  140 XPOS=80*SINRADC%
  150 YPOS=30*SINRADD%
  160 ZPOS=220-80*COSRADC%
  170 PROCINIT
  180 REPEAT
  190 PROCROTATE
  200 SP%=3-SP%:WAIT
  210 OSCLI"FX113,"+STR$SP%
  220 OSCLI"FX112,"+STR$(3-SP%)
  230 CLS
  240 PROCPLOT
  250 XROT+=5:YROT+=4
  260 C%=(C%+5)MOD360
  270 D%=(D%+4)MOD360
  280 XPOS=80*SINRADC%
  290 YPOS=30*SINRADD%
  300 ZPOS=220-80*COSRADC%
  310 UNTIL FALSE
  320 END
  330 :
  340 DEFPROCINIT
  350 coords=84:lines=168
  360 RESTORE 700
  370 FOR X=1 TO coords
  380 READ XW(X),YW(X),ZW(X):NEXT
  390 FOR X=1 TO lines
  400 READ plot(X),point(X):NEXT
  410 ENDPROC
  420 
  430 DEFPROCROTATE
  440 FOR I%=1 TO coords
  450 XA=XW(I%):YA=YW(I%):ZA=ZW(I%)
  460 XR=XA*COS(RAD(ZROT))-YA*SIN(RAD(ZROT))
  470 YA=YA*COS(RAD(ZROT))+XA*SIN(RAD(ZROT))
  480 XA=XR
  490 XR=XA*COS(RAD(YROT))-ZA*SIN(RAD(YROT))
  500 ZA=ZA*COS(RAD(YROT))+XA*SIN(RAD(YROT))
  510 XA=XR
  520 YR=YA*COS(RAD(XROT))-ZA*SIN(RAD(XROT))
  530 ZA=ZA*COS(RAD(XROT))+YA*SIN(RAD(XROT))
  540 YA=YR
  550 XR(I%)=XA+XPOS:YR(I%)=YA+YPOS:ZR(I%)=ZA+ZPOS
  560 XS(I%)=800*XR(I%)/ZR(I%)
  570 YS(I%)=800*YR(I%)/ZR(I%)
  580 NEXT:ENDPROC
  590 :
  600 DEFPROCPLOT
  610 FOR P%=1 TO lines
  620 PLOT plot(P%),XS(point(P%)),YS(point(P%))
  630 NEXT:ENDPROC
  640 
  650 *FX112,1
  660 *FX113,1
  670 ON:REPORT:PRINT" at line "ERL
  680 
