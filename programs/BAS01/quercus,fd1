   10 REM > QUERCUS
   20 
   30 SYS"OS_UpdateMEMC",64,64
   40 ONERRORGOTO590
   50 DIM XW(600),YW(600),ZW(600)
   60 DIM XR(600),YR(600),ZR(600)
   70 DIM XS(600),YS(600)
   80 DIM plot(600),point(600)
   90 MODE0:MODE128:SP%=1
  100 ORIGIN 640,512:OFF
  110 COLOUR 0,4
  120 XROT=0:YROT=0:ZROT=0
  130 XPOS=0
  140 YPOS=0
  150 ZPOS=300
  160 PROCINIT
  170 REPEAT
  180 PROCROTATE
  190 SP%=3-SP%:WAIT
  200 OSCLI"FX113,"+STR$SP%
  210 OSCLI"FX112,"+STR$(3-SP%)
  220 CLS
  230 PROCPLOT
  240 XROT+=9:YROT+=7
  250 UNTIL FALSE
  260 END
  270 :
  280 DEFPROCINIT
  290 coords=268:lines=536
  300 RESTORE 650
  310 FOR X=1 TO coords
  320 READ XW(X),YW(X),ZW(X):NEXT
  330 FOR X=1 TO lines
  340 READ plot(X),point(X):NEXT
  350 ENDPROC
  360 
  370 DEFPROCROTATE
  380 FOR I%=1 TO coords
  390 XA=XW(I%):YA=YW(I%):ZA=ZW(I%)
  400 XR=XA*COS(RAD(ZROT))-YA*SIN(RAD(ZROT))
  410 YA=YA*COS(RAD(ZROT))+XA*SIN(RAD(ZROT))
  420 XA=XR
  430 XR=XA*COS(RAD(YROT))-ZA*SIN(RAD(YROT))
  440 ZA=ZA*COS(RAD(YROT))+XA*SIN(RAD(YROT))
  450 XA=XR
  460 YR=YA*COS(RAD(XROT))-ZA*SIN(RAD(XROT))
  470 ZA=ZA*COS(RAD(XROT))+YA*SIN(RAD(XROT))
  480 YA=YR
  490 XR(I%)=XA+XPOS:YR(I%)=YA+YPOS:ZR(I%)=ZA+ZPOS
  500 XS(I%)=800*XR(I%)/ZR(I%)
  510 YS(I%)=800*YR(I%)/ZR(I%)
  520 NEXT:ENDPROC
  530 :
  540 DEFPROCPLOT
  550 FOR P%=1 TO lines
  560 PLOT plot(P%),XS(point(P%)),YS(point(P%))
  570 NEXT:ENDPROC
  580 
  590 *FX112,1
  600 *FX113,1
  610 ON:REPORT:PRINT" at line "ERL
  620 END
  630 
