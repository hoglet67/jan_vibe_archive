   10 REM > NAVN3D
   20 
   30 MODE0
   40 DIM XS(1000),YS(1000),ZS(1000),PL(1000)
   50 
   60 ORIGIN 640,512
   70 CP%=0:Z%=4
   80 
   90 INPUT"Indtast tekst "A$
  100 FX%=6*LENA$*Z%:FY%=6*Z%
  110 
  120 XP%=0:YP%=0
  130 FORN%=1TOLENA$
  140 RESTORE 650:B$=MID$(A$,N%,1)
  150 REPEAT:READC$:UNTILC$=B$
  160 REPEAT:READ P%,X%,Y%
  170 X%=XP%+Z%*X%-FX%:Y%=YP%+Z%*Y%-FY%
  180 IF P%<>0 THEN
  190 PLOTP%,X%+Z%/2,Y%+Z%/2
  200 CP%=CP%+1:PL(CP%)=P%:XS(CP%)=X%:YS(CP%)=Y%:ZS(CP%)=Z%
  210 ENDIF
  220 UNTILP%=0
  230 XP%=XP%+Z%*12
  240 NEXT
  250 
  260 XP%=0:YP%=0:CP1%=CP%
  270 FORN%=1TOLENA$
  280 RESTORE 650:B$=MID$(A$,N%,1)
  290 REPEAT:READC$:UNTILC$=B$
  300 REPEAT:READ P%,X%,Y%
  310 X%=XP%+Z%*X%-FX%:Y%=YP%+Z%*Y%-FY%
  320 IF P%<>0 THEN
  330 PLOTP%,X%,Y%
  340 CP%=CP%+1:PL(CP%)=P%:XS(CP%)=X%:YS(CP%)=Y%:ZS(CP%)=-Z%
  350 ENDIF
  360 UNTILP%=0
  370 XP%=XP%+Z%*12
  380 NEXT
  390 
  400 *SPOOL 3DN
  410 PRINT"AUTO 5000"
  420 PRINT"REM Punkter:";CP%
  430 FOR N%=1TOCP%
  440 IFN%MOD4=1 PRINT"DATA";
  450 PRINTSTR$XS(N%)","STR$YS(N%)","STR$ZS(N%);
  460 IFN%MOD4=0 PRINT ELSE PRINT",  ";
  470 NEXT:PRINT:PRINT" "
  480 
  490 PRINT"REM Plot:"2*CP%
  500 FORN%=1TOCP%
  510 IFN%MOD8=1 PRINT"DATA";
  520 PRINTSTR$PL(N%)","STR$N%;
  530 IFN%MOD8=0 PRINT ELSE PRINT",  ";
  540 NEXT:PRINT:PRINT" "
  550 
  560 FORN%=1TOCP1%:N1%=N%+CP1%
  570 IFN%MOD4=1 PRINT"DATA";
  580 PRINT"4,"STR$N%",  5,"STR$N1%;
  590 IFN%MOD4=0 PRINT ELSE PRINT",  ";
  600 NEXT:PRINT
  610 *SPOOL
  620 
  630 END
