   10 REM *** PROCDUMP *** CP80 SCREENDUMP (MODE0) *** LIBRARY DUMP0 ***
   20 
   30 DEFPROCDUMP
   40 LOCAL X%,Y%,A%,B%
   50 ORIGIN 0,0
   60 VDU2,1,27,1,51,1,21
   70 FORY%=1008TO0STEP-16
   80 VDU1,27,1,75,1,128,1,2
   90 FORX%=0TO1278STEP2:A%=0:B%=%11
  100 FORI%=0TO12STEP4
  110 IFPOINT(X%,Y%+I%)=1 A%=A%+B%
  120 B%=B%<<2:NEXT:VDU1,A%:NEXT
  130 NEXT:VDU1,27,1,64,3
  140 ENDPROC
