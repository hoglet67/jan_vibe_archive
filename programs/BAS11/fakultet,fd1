   10 REM > FAKULTET
   20 MODE31:OFF
   30 DIM A%(10000):A%(1)=1:K%=1:INPUT"Number="B%
   40 FORN%=1TOB%:C%=0:VDU31,0,1:PRINTN%
   50 FORM%=1TOK%:A%(M%)=A%(M%)*N%+C%:C%=A%(M%)DIV100000:A%(M%)=A%(M%)MOD100000:NEXT
   60 IFC%<>0 K%+=1:A%(K%)=C%
   70 NEXT:PRINT STR$(A%(K%));
   80 IF K%>1 FORN%=K%-1TO1STEP-1:PRINTRIGHT$(STR$(100000+A%(N%)),5);:NEXT
   90 PRINT
