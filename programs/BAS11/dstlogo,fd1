   10 REM > DSTLOGO
   20 
   30 MODE31
   40 
   50 DIM PATH% 5000:FX=1.2:FY=1.2:DX=128:DY=64:K%=256
   60 
   70 GCOL7:RECTANGLEFILL 0,0,1600,1200
   80 FORN%=1TO4:READX1%,Y1%,X2%,Y2%
   90 PP%=0
  100 X1%=X1%*FX+DX:Y1%=Y1%*FY+DY
  110 X2%=X2%*FX+DX:Y2%=Y2%*FY+DY
  120 PROCM(X1%,Y1%):PROCD(X1%,Y2%):PROCD(X2%,Y2%):PROCD(X2%,Y1%):PROCC:PROCE
  130 GCOL0:SYS"Draw_Fill",PATH%
  140 NEXT
  150 PP%=0
  160 READ X%,Y%:PROCM(X%*FX+DX,Y%*FY+DY)
  170 FORN%=1TO121:READ X%,Y%:PROCD(X%*FX+DX,Y%*FY+DY):NEXT
  180 PROCC:PROCE
  190 GCOL2:SYS"Draw_Fill",PATH%
  200 GCOL0:SYS"Draw_Stroke",PATH%
  210 END
  220 
  230 DEFPROCM(X%,Y%)
  240 X%=X%*K%:Y%=Y%*K%
  250 PATH%!PP%=2:PP%+=4:PATH%!PP%=X%:PP%+=4:PATH%!PP%=Y%:PP%+=4
  260 ENDPROC
  270 
  280 DEFPROCD(X%,Y%)
  290 X%=X%*K%:Y%=Y%*K%
  300 PATH%!PP%=8:PP%+=4:PATH%!PP%=X%:PP%+=4:PATH%!PP%=Y%:PP%+=4
  310 ENDPROC
  320 
  330 DEFPROCC
  340 PATH%!PP%=5:PP%+=4
  350 ENDPROC
  360 
  370 DEFPROCE
  380 PATH%!PP%=0:PP%+=4:PATH%!PP%=PP%
  390 ENDPROC
