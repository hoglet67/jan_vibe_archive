   10 REM > DSTLOGO2
   20 
   30 MODE31
   40 
   50 DIM PATH% 5000:FX=1.2:FY=1.2:DX=128:DY=64:K%=256
   60 
   70 PP%=0
   80 READ X%,Y%:PROCM(X%*FX+DX,Y%*FY+DY)
   90 FORN%=1TO121:READ X%,Y%:PROCD(X%*FX+DX,Y%*FY+DY):NEXT
  100 PROCC:PROCE
  110 GCOL2:SYS"Draw_Fill",PATH%
  120 FORX%=0TO1600STEP2
  130 Y1%=0:Y2%=1198
  140 REPEAT:Y1%=Y1%+2:UNTIL POINT(X%,Y1%)<>0
  150 REPEAT:Y2%=Y2%-2:UNTIL POINT(X%,Y2%)<>0
  170 IF Y1%<Y2% THEN
  180 YM%=(Y1%+Y2%)/2:MOVE X%,Y1%:MOVE X%,Y2%:PLOT&BD,X%,600-YM%+Y1%
  190 ENDIF
  200 NEXT
  210 END
  220 
  230 DEFPROCM(X%,Y%)
  240 X%=X%*K%:Y%=Y%*K%
  250 PATH%!PP%=2:PP%+=4:PATH%!PP%=X%:PP%+=4:PATH%!PP%=Y%:PP%+=4
  260 ENDPROC
  270 
  280 DEFPROCD(X%,Y%)
  290 X%=X%*K%:Y%=Y%*K%
  300 PATH%!PP%=8:PP%+=4:PATH%!PP%=X%:PP%+=4:PATH%!PP%=Y%:PP%+=4
  310 ENDPROC
  320 
  330 DEFPROCC
  340 PATH%!PP%=5:PP%+=4
  350 ENDPROC
  360 
  370 DEFPROCE
  380 PATH%!PP%=0:PP%+=4:PATH%!PP%=PP%
  390 ENDPROC
  400 
