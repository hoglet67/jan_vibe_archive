   10 REM > MAZE
   20 
   30 MODE31:OFF:CIRCLEFILL800,600,100
   40 REPEAT:X%=RND(1600):Y%=RND(1200):UNTILPOINT(X%,Y%)=0
   50 PROCA(X%,Y%):GCOL0:CIRCLE FILL 800,600,100:VDU31,49,37:PRINT"Mål";:VDU30
   60 END
   70 
   80 DEFPROCA(X%,Y%)
   90 LOCAL X1%,Y1%,N%
  100 FORN%=1TO8:X1%=12*(RND(3)-2)+X%:Y1%=12*(RND(3)-2)+Y%
  110 IF POINT(X1%,Y1%)=0 AND POINT((X%+X1%)/2,(Y%+Y1%)/2)=0 LINE X%,Y%,X1%,Y1%:PROCA(X1%,Y1%)
  120 NEXT:ENDPROC
