   10 REM > COLSPIR
   20 
   30 MODE15:OFF
   40 
   50 FORI%=0TO220:FORN%=0TO26:T=RAD(13.33*N%+I%):PROCF(N%,I%+N%)
   60 CIRCLE FILL 640+4*I%*SINT,512+4*I%*COST,I%/5+8:NEXT,:END
   70 
   80 DEFPROCF(F%,I%)
   90 LOCAL R%,G%,B%
  100 I%=ABS(6-I%MOD12):B%=F%DIV9:F%=F%MOD9:G%=F%DIV3:F%=F%MOD3:R%=F%
  110 F%=R%+4*G%+16*B%:IF I%>=4 F%=F%+&15
  120 GCOL F% TINT (64*I%)MOD256
  130 ENDPROC
