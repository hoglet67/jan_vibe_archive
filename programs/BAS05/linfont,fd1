   10 REM > LINFONT
   20 
   30 MODE0:COLOUR 0,0,0,255
   40 
   50 PROCTXT(0,800,1279,950,"NORMAL TEKST")
   60 PROCTXT(0,750,1279,600,"P} HOVEDET OG SPEJLVENDT")
   70 PROCTXT(1279,400,0,550,"SPEJLVENDT")
   80 PROCTXT(1279,350,0,200,"P} HOVEDET")
   90 PROCTXT(32,32,1248,96,"ABCDEFGHIJKLMNOPQRSTUVWXYZ{|}0123456789")
  100 
  110 END
  120 
  130 DEFPROCTXT(X1,Y1,X2,Y2,TXT$)
  140 LOCAL O%(),X(),Y(),I%,N%,K%,L%,A$,B$,ZX,ZY,H
  150 DIM O%(20),X(20),Y(20)
  160 L%=LENTXT$:ZY=Y2-Y1:H=(X2-X1)/(5*L%):ZX=(X2-X1+H)/L%
  170 FORI%=1TOL%
  180 A$=MID$(TXT$,I%,1)
  190 IF INSTR("ABCDEFGHIJKLMNOPQRSTUVWXYZ{|}0123456789",A$) THEN
  200 IF INSTR("0123456789",A$) A$="'"+A$+"'"
  210 RESTORE 340
  220 REPEAT:READ B$:UNTIL A$=B$:READ K%
  230 FORN%=1TOK%:READ O%(N%),X(N%),Y(N%):X(N%)=X(N%)/5:Y(N%)=Y(N%)/6:NEXT
  240 FORN%=1TOK%
  250 CASE O%(N%) OF
  260 WHEN 0: MOVE X1+ZX*X(N%),Y1+ZY*Y(N%)
  270 WHEN 1: DRAW X1+ZX*X(N%),Y1+ZY*Y(N%)
  280 ENDCASE
  290 NEXT
  300 ENDIF
  310 X1+=ZX
  320 NEXT:ENDPROC
  330 
