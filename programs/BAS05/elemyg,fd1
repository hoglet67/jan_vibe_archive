   10 REM > ELEMYG
   20 
   30 MODE128:MODE0:OFF
   40 S1%=1:S2%=2:P1%=1:P2%=2:A%=47:D%=50
   50 SYS 6,112,S1%:SYS 6,113,S2%:SWAP S1%,S2%
   60 ONERRORGOTO260
   70 
   80 DIM X%(2,A%),Y%(2,A%),XP%(A%),YP%(A%)
   90 FORN%=1TOA%:READ X%(1,N%),Y%(1,N%):NEXT
  100 FORN%=1TOA%:READ X%(2,N%),Y%(2,N%):NEXT
  110 
  120 REPEAT
  130   FORN%=0TOD%
  140     FORI%=1TOA%
  150       XP%(I%)=(X%(P1%,I%)*(D%-N%)+X%(P2%,I%)*N%)/D%
  160       YP%(I%)=(Y%(P1%,I%)*(D%-N%)+Y%(P2%,I%)*N%)/D%
  170     NEXT
  180     MOVE XP%(1),YP%(1)
  190     FORI%=2TOA%:DRAW XP%(I%),YP%(I%):NEXT
  200     SYS 6,112,S1%:SYS 6,113,S2%:WAIT:CLS:SWAP S1%,S2%
  210   NEXT
  220   T%=TIME+100:REPEAT UNTIL TIME>=T%
  230   SWAP P1%,P2%
  240 UNTIL0
  250 
  260 SYS 6,112,S1%:REPORT:PRINT" at line "STR$ERL:ON:END
