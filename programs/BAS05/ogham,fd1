   10 REM > OGHAM
   20 
   30 MODE0:COLOUR0,0,0,255
   40 REPEAT
   50 X%=32:Y%=900:S%=4
   60 VDU31,0,29:INPUT"TEKST="A$:IF RIGHT$(A$,1)<>" " A$=A$+" "
   70 CLS:PRINTA$":"
   80 FORN%=1TOLENA$:B$=MID$(A$,N%,1)
   90   IF INSTR(" ABCDEFGHIJKLMNOPQRSTUVWXYZ",B$)=0 B$=" "
  100   RESTORE:REPEAT:READL$:UNTIL L$=B$:READ K%,W%
  110   IF X%+S%*W%>1240:LINE X%,Y%,X%+S%*4,Y%:X%=32:Y%-=24*S%
  120   LINEX%,Y%,X%+S%*W%,Y%
  130   FORI%=1TOK%:READ X1%,Y1%,X2%,Y2%
  140     LINE S%*X1%+X%,S%*Y1%+Y%,S%*X2%+X%,S%*Y2%+Y%
  150   NEXT:X%+=S%*W%
  160 NEXT
  170 UNTIL0
  180 
