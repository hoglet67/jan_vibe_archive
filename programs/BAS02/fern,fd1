   10 REM > FERN
   20 
   30 MODE0:PROCA(128,0,220,30,1):END
   40 
   50 DEFPROCA(X,Y,Z,A,M)
   60 IFZ<=2 ENDPROC
   70 LOCAL X1,Y1
   80 X1=X+Z*SINRADA:Y1=Y+Z*COSRADA
   90 LINEX,Y,X1,Y1
  100 PROCA(X1,Y1,.85*Z,A+3*M,M)
  110 PROCA(X1,Y1,.33*Z,A-40*M,M)
  120 PROCA(X1,Y1,.33*Z,A+60*M,-M)
  130 ENDPROC
