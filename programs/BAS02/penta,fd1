  460 
  470 DEFPROCP(X1%,Y1%,X2%,Y2%,X3%,Y3%,X4%,Y4%,X5%,Y5%,N1%,N2%,C%)
  480 LOCAL X6%,Y6%,X7%,Y7%,X8%,Y8%,X9%,Y9%,X0%,Y0%,D%
  490 GCOL (C%+4)MOD10+1:D%=N1%+N2%
  500 MOVEX5%,Y5%:DRAWX1%,Y1%:DRAWX2%,Y2%:DRAWX3%,Y3%:DRAWX4%,Y4%:DRAWX5%,Y5%
  510 IFC%=1ENDPROC
  520 X6%=(N1%*X1%+N2%*X2%)/D%:Y6%=(N1%*Y1%+N2%*Y2%)/D%
  530 X7%=(N1%*X2%+N2%*X3%)/D%:Y7%=(N1%*Y2%+N2%*Y3%)/D%
  540 X8%=(N1%*X3%+N2%*X4%)/D%:Y8%=(N1%*Y3%+N2%*Y4%)/D%
  550 X9%=(N1%*X4%+N2%*X5%)/D%:Y9%=(N1%*Y4%+N2%*Y5%)/D%
  560 X0%=(N1%*X5%+N2%*X1%)/D%:Y0%=(N1%*Y5%+N2%*Y1%)/D%
  570 PROCP(X6%,Y6%,X7%,Y7%,X8%,Y8%,X9%,Y9%,X0%,Y0%,N1%,N2%,C%-1)
  580 ENDPROC
  590 
  600 DEFPROCT(X1%,Y1%,X2%,Y2%,X3%,Y3%,N1%,N2%,C%)
  610 LOCAL X4%,Y4%,X5%,Y5%,X6%,Y6%,D%
  620 GCOL C%:D%=N1%+N2%
  630 MOVEX3%,Y3%:DRAWX1%,Y1%:DRAWX2%,Y2%:DRAWX3%,Y3%
  640 IFC%=1ENDPROC
  650 X4%=(N1%*X1%+N2%*X2%)/D%:Y4%=(N1%*Y1%+N2%*Y2%)/D%
  660 X5%=(N1%*X2%+N2%*X3%)/D%:Y5%=(N1%*Y2%+N2%*Y3%)/D%
  670 X6%=(N1%*X3%+N2%*X1%)/D%:Y6%=(N1%*Y3%+N2%*Y1%)/D%
  680 PROCT(X4%,Y4%,X5%,Y5%,X6%,Y6%,N1%,N2%,C%-1)
  690 ENDPROC
