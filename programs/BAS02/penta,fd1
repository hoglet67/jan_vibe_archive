   10 REM > PENTA
   20 
   30 SYS"OS_UpdateMEMC",64,64
   40 MODE140:MODE12:OFF:ORIGIN 640,512
   50 ONERRORGOTO440
   60 
   70 FORN%=1TO10
   80 F1%=36*N%:F2%=F1%+120:F3%=F1%+240
   90 C1%=128*(SINRADF1%+1)
  100 C2%=128*(SINRADF2%+1)
  110 C3%=128*(SINRADF3%+1)
  120 COLOUR N%,C1%,C2%,C3%
  130 NEXT
  140 
  150 DIM PX%(10),PY%(10)
  160 K%=500:S1$="1":S2$="2"
  170 FORN%=1TO10:T=RAD(36*N%):K%=690-K%
  180 PX%(N%)=K%*SINT:PY%(N%)=K%*COST:NEXT
  190 MOVEPX%(10),PY%(10)
  200 FORN%=1TO10:DRAWPX%(N%),PY%(N%):NEXT
  210 
  220 S%=60:A%=5
  230 REPEAT
  240 FORN%=1TOS%:N1%=S%-N%
  250 OSCLI"FX112,"+S1$:OSCLI"FX113,"+S2$:WAIT:SWAP S1$,S2$:CLS
  260 
  270 PROCT(PX%(1),PY%(1),PX%(2),PY%(2),PX%(3),PY%(3),N1%,N%,A%)
  280 PROCT(PX%(3),PY%(3),PX%(4),PY%(4),PX%(5),PY%(5),N1%,N%,A%)
  290 PROCT(PX%(5),PY%(5),PX%(6),PY%(6),PX%(7),PY%(7),N1%,N%,A%)
  300 PROCT(PX%(7),PY%(7),PX%(8),PY%(8),PX%(9),PY%(9),N1%,N%,A%)
  310 PROCT(PX%(9),PY%(9),PX%(10),PY%(10),PX%(1),PY%(1),N1%,N%,A%)
  320 
  330 PROCT(PX%(2),PY%(2),PX%(3),PY%(3),PX%(4),PY%(4),N1%,N%,A%)
  340 PROCT(PX%(4),PY%(4),PX%(5),PY%(5),PX%(6),PY%(6),N1%,N%,A%)
  350 PROCT(PX%(6),PY%(6),PX%(7),PY%(7),PX%(8),PY%(8),N1%,N%,A%)
  360 PROCT(PX%(8),PY%(8),PX%(9),PY%(9),PX%(10),PY%(10),N1%,N%,A%)
  370 PROCT(PX%(10),PY%(10),PX%(1),PY%(1),PX%(2),PY%(2),N1%,N%,A%)
  380 
  390 PROCP(PX%(1),PY%(1),PX%(3),PY%(3),PX%(5),PY%(5),PX%(7),PY%(7),PX%(9),PY%(9),N%,N1%,10)
